            <Name>Medal</Name>
            <FileName>dummy.set.xml</FileName>
          </OverWriteSetData>
          <OverWriteSetData>
            <Name>Media</Name>
            <FileName>dummy.set.xml</FileName>
          </OverWriteSetData>
          <!-- ここからはスコア情報 -->
