	<!-- 国ごとの共通化 -->
	<Node>
		<Order>-100</Order>
		<UsageDefault>Stage</UsageDefault>
		<Archive>CmnCountry</Archive>
		<Param>Country</Param>
		<Node>
			<Archive>Cmn%Country%</Archive>
			<Tag>
				<Key>Country</Key>
				<Value>%Country%</Value>
			</Tag>
			<Tag>
				<Key>CountryTown</Key>
				<Value>%CountryTown%</Value>
			</Tag>
			<Node>
				<Archive>CmnAct_%Country%</Archive>
				<Tag>
					<Key>Type</Key>
					<Value>Act</Value>
				</Tag>
				<Node>
					<Archive>CmnActD_Terrain_%Country%</Archive>
				</Node>
				<Node>
					<Archive>CmnActN_Terrain_%Country%</Archive>
				</Node>
			</Node>
			<Node>
				<Tag>
					<Key>Type</Key>
					<Value>Town</Value>
				</Tag>
				<UsageDefault>Stage</UsageDefault>
				<Archive>CmnTown_%CountryTown%</Archive>
				<!-- Townは共有状況にかかわらず全部Cmn で共有-->
				<Node>
					<Archive>CmnTown_Terrain_%CountryTown%</Archive>
					<Node>
						<Archive>Town_%CountryTown%</Archive>
						<DefAppend>TownD</DefAppend>
						<Append>Town_%CountryTown%_Common</Append>
					</Node>
				</Node>
				<Node>
